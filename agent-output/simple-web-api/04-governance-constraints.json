{
  "projectName": "simple-web-api",
  "environment": "dev",
  "region": "swedencentral",
  "budget": {
    "monthly": 100,
    "currency": "USD",
    "type": "hard"
  },
  "policies": {
    "naming": {
      "convention": "CAF",
      "uniqueSuffix": true
    },
    "tags": {
      "required": ["Environment", "Project", "ManagedBy", "Owner"],
      "defaults": {
        "ManagedBy": "Bicep"
      }
    },
    "security": {
      "httpsOnly": true,
      "minimumTlsVersion": "TLS1_2",
      "disablePublicBlobAccess": true,
      "preferManagedIdentity": true
    },
    "network": {
      "privateEndpointsRequired": false,
      "vnetIntegrationRequired": false,
      "publicEndpointsAllowed": true
    }
  },
  "skuRestrictions": {
    "staticWebApps": "Free",
    "functions": "Consumption",
    "cosmosDb": "Serverless",
    "applicationInsights": "PayAsYouGo"
  },
  "compliance": {
    "frameworks": [],
    "dataResidency": {
      "primaryRegion": "swedencentral",
      "crossRegionReplication": false
    }
  }
}
